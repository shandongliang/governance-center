  "scripts": {
    "dev": "webpack-dev-server --color --progress --config build/webpack.dev.conf.js",
    "build": "npm run cleanAndCopy && webpack --progress --config build/webpack.prod.conf.js",
    "cleanAndCopy": "rimraf ./dist/* && node ./build/copyassets.js -d ./dist/assets -s ./assets",
    "generate": "node ./build/generate.base.project.js && npm run copy-src-ns",
    "copy-src-ns": "node ./build/copyassets.js -d ./pandora-web/src -s ./src_ns",
    "win-pandora-update": "ncp node_modules/cmbc-pandora-web/src src",
    "prewin-pandora-update": "npm install --save-dev cmbc-pandora-web",
    "postwin-pandora-update": "echo pandora update finished ",
    "start": "nodemon --watch config/userConfig.js --exec npm run dev"
